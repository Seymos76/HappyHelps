{% extends "base.html.twig" %}

{% block body %}
    <h1>Register</h1>

    {{ form_start(form) }}
    {{ form_row(form.firstname) }}
    {{ form_row(form.lastname) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}
    <button type="submit">Submit</button>
    {{ form_end(form) }}
{% endblock %}